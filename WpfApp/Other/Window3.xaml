<Window x:Class="WpfApp.Other.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:WpfApp.Other"
        mc:Ignorable="d"
        Title="Window3" Height="450" Width="800">
    <Window.CommandBindings>
        <CommandBinding Command="Close" Executed="CommandBinding_OnExecuted" CanExecute="CommandBinding_OnCanExecute"/>
    </Window.CommandBindings>
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Width="100" Height="100" Command="Close">Close</Button>
    </StackPanel>
</Window>
